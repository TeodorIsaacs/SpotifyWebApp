{
  "extends": [ "eslint:recommended", "plugin:react/recommended" ],
  "parserOptions": {
      ecmaVersion: 7,
      ecmaFeatures: {
          jsx: true,
      },
      sourceType: "module"
  },
  "env": {
    "browser": true,
    "node": true
  },
  rules: {
    'no-console': 'off'
  },
  "plugins": [ "react" ]
}